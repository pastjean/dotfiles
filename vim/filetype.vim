runtime! ftdetect/*.vim

au BufRead,BufNewFile *.zsh        set filetype=zsh
au BufRead,BufNewFile *.gnuplot    set filetype=gnuplot
au BufRead,BufNewFile *.mustache   set filetype=mustache
au BufRead,BufNewFile *.{md,mkd,mkdn,mark*}   set filetype=markdown
au BufRead,BufNewFile *.less   set filetype=less
au BufRead,BufNewFile config.ru,watch.me   set filetype=ruby


